/**index.wxss**/
.container{
  background: url('https://game.flyh5.cn/resources/game/wechat/tzj/img/gugong20190719/mybj.png') no-repeat;
  background-size: cover;
}
.prizeBox{width:90%;height:640rpx;border-radius: 20rpx;padding:20rpx; background: #c0a06f;margin:50rpx auto 0;}
.prizeBox .prize_box{position: relative;}
.prizeBox .ul .li,.start{width: 31%;height:200rpx;text-align: center;border-radius: 20rpx;border:1rpx solid #ccc;box-sizing: border-box;font-size: 30rpx;display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;}
.prizeBox .ul .li{background:#ffe7d0; color: #915b2f}
.prizeBox .ul .li.cur{background: #fff;color:#915b2f;border-color:#fff;}
.prizeBox .ul .li:nth-child(2){position: absolute;left:34.5%;top:0;}
.prizeBox .ul .li:nth-child(3){position: absolute;left:69%;top:0;}
.prizeBox .ul .li:nth-child(4){position: absolute;right:0;top:220rpx;}
.prizeBox .ul .li:nth-child(5){position: absolute;right:0;top:440rpx;}
.prizeBox .ul .li:nth-child(6){position: absolute;right:34.5%;top:440rpx;}
.prizeBox .ul .li:nth-child(7){position: absolute;right:69%;top:440rpx;}
.prizeBox .ul .li:nth-child(8){position: absolute;left:0;top:220rpx;}
.start{position: absolute;left:34.5%;top:220rpx;background: #8481FF;color:#fff;border-color:#8481FF;}
.prizeBox .ul .li image{
  width: 90%;
  display: block;
}
.start.on{background: #B92722;border-color:#B92722;}
.num{ line-height: 136rpx; text-align: center; color: #b0824d; font-size: 30rpx;}
.num span{ font-size: 60rpx;}
.sta_btn{
  width: 590rpx;
  height: 120rpx;
  background: url('https://game.flyh5.cn/resources/game/wechat/tzj/img/gugong20190719/btnsss.png') no-repeat;
  background-size: 100% 100%;
  margin: auto;
  text-align: center;
  color: #543808;
  font-size: 36rpx;
  line-height: 120rpx;
}
.sta_btn span{
  font-size: 24rpx;
}
.sta_btn1{
  width: 636rpx;
  margin: auto;
  margin-top: 40rpx;
}
.sta_btn1 .sta_btn1_a{
  flex: 0 300rpx;
  background: url('https://game.flyh5.cn/resources/game/wechat/tzj/img/gugong20190719/jiang_btn1.png') no-repeat;
  background-size: 100% 100%;
  height:82rpx;
  line-height: 82rpx;
  text-align: center;
  color: #ac804a;
  font-size: 36rpx;
}
.head{
  padding: 36rpx 20rpx 0 20rpx;
}
.head .img1{
  width: 80rpx;
  height: 80rpx;
}
.head .img2{
  width: 220rpx;
  height: 80rpx;
}
.popup{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 2;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
}
.jianggongs{
  width: 520rpx;
  height: 640rpx;
  background: url('https://game.flyh5.cn/resources/game/wechat/tzj/img/gugong20190719/zan.png') no-repeat;
  background-size: 100% 100%;
}
.jianggongs .btn{
  width: 434rpx;
  height: 88rpx;
  background: url('https://game.flyh5.cn/resources/game/wechat/tzj/img/gugong20190719/btnsss.png') no-repeat;
  background-size: 100% 100%;
  margin: auto;
  line-height: 88rpx;
  text-align: center;
  color: #543808;
  font-size: 42rpx;
  margin-top: 470rpx;
}